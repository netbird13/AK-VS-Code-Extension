	"navbar with router link": {
		"prefix": "ak-navbar",
		"body": [
			"Navigation",
			"<nav class=\"navbar navbar-default navbar-fixed-top\" role=\"navigation\">",
			"<ul class=\"nav navbar-nav\">",
			"<li>",
			"<a [routerLink]=\"['/home']\" routerLinkActive=\"router-link-active\">Home</a>",
			"</li>",
			"<a>",
			"{{time | date: 'EEEE dd-MMM-yyyy HH:mm'}}",
			"</a>",
			"</ul>",
			"</nav>"
		],
		"description": "Create navbar with router-link and piped time"
	},

	"header & router outlet": {
		"prefix": "ak-app-component",
		"body": ["<header>",
		"<app-navbar></app-navbar>",
	 "</header>",
	 "<br>",
	 "<main>",
		 "<router-outlet></router-outlet>",
	 "</main>"],
		"description": "Adds header with navbar and main with router-outlet to app.component.html"
	}

}